{
	"name": "search-in-browser",
	"displayName": "search-in-browser",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		],
		"keybindings": [
			{
				"key": "alt+shift+s",
				"command": "extension.helloWorld",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "5.2.6",
		"@types/node": "10.14.1",
		"@types/valid-url": "1.0.2",
		"tslint": "5.13.1",
		"typescript": "3.3.3333",
		"vscode": "1.1.30"
	},
	"dependencies": {
		"@types/opn": "^5.1.0",
		"opn": "^5.4.0",
		"valid-url": "^1.0.9"
	}
}
